    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.8",
    "consola": "^2.15.3",
    "cz-git": "^1.3.10",
    "czg": "^1.3.10",
    "eslint": "^8.21.0",
    "eslint-plugin-markdown": "^3.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
