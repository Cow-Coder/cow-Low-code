    "@vueuse/integrations": "^9.0.2",
    "element-plus": "^2.2.12",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.33.0",
    "pinia": "^2.0.16",
    "pinia-plugin-persist": "^1.0.0",
    "uuid": "^8.3.2",
    "vant": "^3.5.3",
    "vue": "^3.2.37",
    "vue-router": "^4.1.2",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
