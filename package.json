    "unplugin-icons": "^0.14.8",
    "unplugin-vue-components": "^0.21.2",
    "vite": "^3.0.1",
    "vite-plugin-inspect": "^0.6.0",
    "vite-plugin-style-import": "^2.0.0",
    "vite-plugin-webpackchunkname": "^0.1.1",
    "vue-tsc": "^0.38.8"
